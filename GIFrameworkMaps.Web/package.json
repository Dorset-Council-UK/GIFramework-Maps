{
  "name": "giframeworkmaps",
  "version": "1.0.0",
  "description": "A .NET based web map built with OpenLayers and Bootstrap",
  "repository": "https://github.com/Dorset-Council-UK/GIFramework-Maps",
  "license": "MIT",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:webpack": "webpack --config webpack.dev.js",
    "watch:webpack": "webpack --config webpack.dev.js --watch",
    "build:webpack-prod": "webpack --config webpack.prod.js",
    "audit": "npm audit",
    "audit fix": "npm audit fix",
    "outdated": "npm outdated",
    "lint": "npx eslint . --ext .ts",
    "prettier": "npx prettier . --write",
    "watch:prettier": "npx onchange \"Scripts/*.ts\" \"wwwroot/css/*.css\" -- prettier --write --ignore-unknown {{changed}}"
  },
  "dependencies": {
    "@camptocamp/ogc-client": "1.2.0",
    "@microsoft/applicationinsights-web": "3.3.5",
    "@microsoft/signalr": "8.0.7",
    "@popperjs/core": "2.11.8",
    "@turf/buffer": "7.2.0",
    "@turf/boolean-intersects": "7.2.0",
    "@turf/kinks": "7.2.0",
    "@turf/unkink-polygon": "7.2.0",
    "accessible-autocomplete": "3.0.1",
    "bootstrap": "5.3.3",
    "dompurify": "3.2.4",
    "fontoxpath": "3.33.0",
    "fuse.js": "7.1.0",
    "geostyler-openlayers-parser": "5.0.1",
    "jspdf": "2.5.2",
    "lodash": "4.17.21",
    "luxon": "3.5.0",
    "nunjucks": "3.2.4",
    "ol": "10.4.0",
    "ol-contextmenu": "5.5.0",
    "ol-mapbox-style": "12.4.1",
    "proj4": "2.15.0",
    "shepherd.js": "13.0.3",
    "sortablejs": "1.15.6",
    "uuid": "11.0.5"
  },
  "devDependencies": {
    "@types/bootstrap": "5.2.10",
    "@types/dom-screen-wake-lock": "1.0.3",
    "@types/dompurify": "3.2.0",
    "@types/lodash": "4.17.15",
    "@types/luxon": "3.4.2",
    "@types/nunjucks": "3.2.6",
    "@types/proj4": "2.5.6",
    "@types/sortablejs": "1.15.8",
    "@typescript-eslint/eslint-plugin": "8.24.0",
    "@typescript-eslint/parser": "8.24.0",
    "eslint": "9.20.1",
    "eslint-config-prettier": "10.0.1",
    "onchange": "7.1.0",
    "prettier": "3.5.0",
    "ts-loader": "9.5.2",
    "typescript": "5.7.3",
    "webpack": "5.97.1",
    "webpack-cli": "6.0.1",
    "webpack-merge": "6.0.1"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "watch:webpack"
    ]
  }
}
