{
  "name": "giframeworkmaps",
  "version": "1.0.0",
  "description": "A .NET based web map built with OpenLayers and Bootstrap",
  "repository": "https://github.com/Dorset-Council-UK/GIFramework-Maps",
  "license": "MIT",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:webpack": "webpack --config webpack.dev.js",
    "watch:webpack": "webpack --config webpack.dev.js --watch",
    "build:webpack-prod": "webpack --config webpack.prod.js",
    "audit": "npm audit",
    "audit fix": "npm audit fix",
    "outdated": "npm outdated",
    "lint": "npx eslint . --ext .ts",
    "prettier": "npx prettier . --write",
    "watch:prettier": "npx onchange \"Scripts/*.ts\" \"wwwroot/css/*.css\" -- prettier --write --ignore-unknown {{changed}}"
  },
  "dependencies": {
    "@microsoft/applicationinsights-web": "3.2.1",
    "@microsoft/signalr": "8.0.0",
    "@popperjs/core": "2.11.8",
    "@turf/buffer": "6.5.0",
    "@turf/intersect": "6.5.0",
    "@turf/kinks": "6.5.0",
    "@turf/line-intersect": "6.5.0",
    "@turf/points-within-polygon": "6.5.0",
    "@turf/unkink-polygon": "6.5.0",
    "accessible-autocomplete": "3.0.0",
    "bootstrap": "5.3.3",
    "dompurify": "3.1.5",
    "fontoxpath": "3.32.0",
    "fuse.js": "7.0.0",
    "jspdf": "2.5.1",
    "lodash": "4.17.21",
    "luxon": "3.4.4",
    "nunjucks": "3.2.4",
    "ol": "9.2.4",
    "ol-contextmenu": "5.4.0",
    "proj4": "2.11.0",
    "shepherd.js": "11.2.0",
    "sortablejs": "1.15.2",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@types/bootstrap": "5.2.10",
    "@types/dom-screen-wake-lock": "1.0.3",
    "@types/dompurify": "3.0.5",
    "@types/lodash": "4.17.4",
    "@types/luxon": "3.4.2",
    "@types/nunjucks": "3.2.6",
    "@types/proj4": "2.5.5",
    "@types/sortablejs": "1.15.8",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "onchange": "7.1.0",
    "prettier": "3.3.1",
    "ts-loader": "9.5.1",
    "typescript": "5.4.5",
    "webpack": "5.91.0",
    "webpack-cli": "5.1.4",
    "webpack-merge": "5.10.0"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "watch:webpack"
    ]
  }
}
